{
	"name": "ch5-react-ts-template",
	"private": true,
	"version": "4.0.0",
	"type": "module",
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"build:dev": "tsc && vite build --mode development",
		"build:prod": "tsc && vite build --mode production",
		"archive": "ch5-cli archive -p ch5-react-ts-template -d dist -o archive -c ./contracts/output/ContractExample/interface/mapping/ContractExample.cse2j",
		"deploy:mobile": "ch5-cli deploy -p -H 172.21.102.150 -t mobile archive/ch5-react-ts-template.ch5z",
		"deploy:xpanel": "ch5-cli deploy -p -H 172.21.102.150 -t web archive/ch5-react-ts-template.ch5z",
		"deploy:panel": "ch5-cli deploy -p -H 0.0.0.0 -t touchscreen archive/ch5-react-ts-template.ch5z --slow-mode",
		"lint": "eslint .",
		"preview": "vite preview"
	},
	"dependencies": {
		"@crestron/ch5-crcomlib": "^2.11.2",
